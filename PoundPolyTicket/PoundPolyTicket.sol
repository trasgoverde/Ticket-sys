// File contracts/PoundPolycat.sol




contract PoundPolycat is
    Initializable,
    ReentrancyGuardUpgradeable,
    UUPSUpgradeable
{
    using SafeERC20 for IERC20;

    IChef public constant MASTER_CHEF =
        IChef(0x8CFD1B9B7478E7B0422916B72d1DB6A9D513D734);

    IERC20 public constant FISH =
        IERC20(0x3a3Df212b7AA91Aa0402B9035b098891d276572B);

    address owner;
    uint256 public constant DFYN_MATIC_FISH = 21;
    uint256 public constant SLIPPAGE = 980; //2%
    mapping(IERC20 => bool) approved;

    struct Farming {
        uint256 totalLocked;
        uint256 lastEarnBlock;
    }

    mapping(IERC20 => Farming) lockedLPs;

    modifier onlyOwner() {
        require(msg.sender == owner, "not owner");
        _;
    }

    function initialize() public initializer {
        __ReentrancyGuard_init();
        owner = msg.sender;
    }

    function _authorizeUpgrade(address) internal override onlyOwner {}

    function deposit(uint256 pid, uint256 amount) external {
        IChef.PoolInfo memory pool = MASTER_CHEF.poolInfo(pid);
        pool.lpToken.safeTransferFrom(msg.sender, address(this), amount);
        if (approved[pool.lpToken] == false) {
            pool.lpToken.safeApprove(address(MASTER_CHEF), type(uint256).max);
            approved[pool.lpToken] = true;
        }
        lockedLPs[pool.lpToken].totalLocked += amount;
        MASTER_CHEF.deposit(pid, amount, owner);
    }

    function withdraw(uint256 pid, uint256 amount) external onlyOwner {
        MASTER_CHEF.withdraw(pid, amount);
        IChef.PoolInfo memory pool = MASTER_CHEF.poolInfo(pid);
        pool.lpToken.transfer(msg.sender, amount);
        lockedLPs[pool.lpToken].totalLocked -= amount;
    }

    function lpToRouter(IERC20 lpToken) public view returns (address) {
        if (
            keccak256(bytes(IERC20Metadata(address(lpToken)).symbol())) ==
            keccak256(bytes("DFYNLP"))
        ) return address(0xA102072A4C07F06EC3B4900FDC4C7B80b6c57429);
        return address(0);
    }

    function earn(uint256 pid) public virtual nonReentrant {
        //get pending rewards
        MASTER_CHEF.deposit(pid, 0, owner);
        uint256 earnedAmt = FISH.balanceOf(address(this));
        require(earnedAmt > 0, "no fish balance");
        IChef.PoolInfo memory pool = MASTER_CHEF.poolInfo(pid);

        address uniRouterAddress = lpToRouter(pool.lpToken);
        require(uniRouterAddress != address(0), "not supported");

        IERC20 token0 = IERC20(UniswapPair(address(pool.lpToken)).token0());
        IERC20 token1 = IERC20(UniswapPair(address(pool.lpToken)).token1());

        IERC20(FISH).safeApprove(uniRouterAddress, 0);
        IERC20(FISH).safeIncreaseAllowance(uniRouterAddress, earnedAmt);

        address[] memory path;
        if (FISH != token0) {
            // Swap half earned to token0
            path[0] = address(FISH);
            path[1] = address(token0);
            _safeSwap(
                uniRouterAddress,
                earnedAmt / 2,
                SLIPPAGE,
                path,
                address(this),
                block.timestamp + 600
            );
        }

        if (FISH != token1) {
            // Swap half earned to token1
            path[0] = address(FISH);
            path[1] = address(token1);
            _safeSwap(
                uniRouterAddress,
                earnedAmt / 2,
                SLIPPAGE,
                path,
                address(this),
                block.timestamp + 600
            );
        }

        // Get want tokens, ie. add liquidity
        uint256 token0Amt = token0.balanceOf(address(this));
        uint256 token1Amt = token1.balanceOf(address(this));
        if (token0Amt > 0 && token1Amt > 0) {
            token0.safeIncreaseAllowance(uniRouterAddress, token0Amt);
            token1.safeIncreaseAllowance(uniRouterAddress, token1Amt);
            IUniswapV2Router02(uniRouterAddress).addLiquidity(
                address(token0),
                address(token1),
                token0Amt,
                token1Amt,
                0,
                0,
                address(this),
                block.timestamp + 600
            );
        }

        lockedLPs[pool.lpToken].lastEarnBlock = block.number;

        uint256 lpBalance = IERC20(pool.lpToken).balanceOf(address(this));
        lockedLPs[pool.lpToken].totalLocked += lpBalance;

        MASTER_CHEF.deposit(pid, lpBalance, owner);
    }

    function _safeSwap(
        address _uniRouterAddress,
        uint256 _amountIn,
        uint256 _slippageFactor,
        address[] memory _path,
        address _to,
        uint256 _deadline
    ) internal virtual {
        uint256[] memory amounts =
            IUniswapV2Router02(_uniRouterAddress).getAmountsOut(
                _amountIn,
                _path
            );
        uint256 amountOut = amounts[amounts.length - 1];

        IUniswapV2Router02(_uniRouterAddress)
            .swapExactTokensForTokensSupportingFeeOnTransferTokens(
            _amountIn,
            (amountOut * _slippageFactor) / 1000,
            _path,
            _to,
            _deadline
        );
    }
}